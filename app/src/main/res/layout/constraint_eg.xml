<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/constraintLayout5">

    <android.support.v7.widget.SwitchCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView11"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout5"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout5"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout5"
        app:layout_constraintHorizontal_bias="0.91"
        app:layout_constraintVertical_bias="0.060000002" />

    <ImageView
        android:layout_width="33dp"
        android:layout_height="33dp"
        app:srcCompat="@drawable/ic_profile"
        android:id="@+id/imageView9"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout5"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout5"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout5"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintVertical_bias="0.060000002" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.08"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout5"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
        android:layout_marginBottom="16dp"
        app:layout_constraintVertical_bias="0.050000012"
        app:layout_constraintRight_toLeftOf="@+id/imageView11"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp" />
    <View
        android:background="@android:color/darker_gray"
        android:id="@+id/view_spacer"
        android:layout_width="0dp"
        android:layout_height="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
        android:layout_marginBottom="16dp"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout5"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout5"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        app:layout_constraintVertical_bias="0.0"></View>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout5"
        android:layout_marginRight="16dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
        android:layout_marginBottom="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout5"
        app:layout_constraintVertical_bias="0.13"
        app:layout_constraintHorizontal_bias="0.08" />

    <Button
        style="@style/Widget.AppCompat.Button.Borderless"
        android:text="Delete"
        android:layout_width="89dp"
        android:layout_height="49dp"
        android:id="@+id/button2"
        app:layout_constraintTop_toBottomOf="@+id/view_spacer"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout5"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout5"
        app:layout_constraintHorizontal_bias="0.92"
        app:layout_constraintVertical_bias="0.060000002" />

    <Button
        style="@style/Widget.AppCompat.Button.Borderless"
        android:text="Edit"
        android:layout_width="89dp"
        android:layout_height="49dp"
        android:id="@+id/button3"
        app:layout_constraintRight_toLeftOf="@+id/button2"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout5"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout5"
        app:layout_constraintTop_toBottomOf="@+id/view_spacer"
        app:layout_constraintHorizontal_bias="0.87"
        app:layout_constraintVertical_bias="0.060000002" />
</android.support.constraint.ConstraintLayout>